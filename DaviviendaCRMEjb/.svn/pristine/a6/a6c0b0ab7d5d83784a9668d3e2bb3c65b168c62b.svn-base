package co.sistemcobro.davivienda.bean;

import java.math.BigDecimal;

public class Rediferido {
	
	private BigDecimal idObligacion;
	private BigDecimal rediferido;
	private BigDecimal saldoDolares;
	private BigDecimal saldoTotal;
	private BigDecimal idRediferido;
	private String rediferidoEspecial;
	private String opcionesRediferido;
	private BigDecimal pagoMinimoInicial;
	private BigDecimal pagoMinDolares;
	private BigDecimal saldoMoraDolares;
	
	public BigDecimal getIdObligacion() {
		return idObligacion;
	}
	public void setIdObligacion(BigDecimal idObligacion) {
		this.idObligacion = idObligacion;
	}
	public BigDecimal getRediferido() {
		return rediferido;
	}
	public void setRediferido(BigDecimal rediferido) {
		this.rediferido = rediferido;
	}
	public BigDecimal getSaldoTotal() {
		return saldoTotal;
	}
	public void setSaldoTotal(BigDecimal saldoTotal) {
		this.saldoTotal = saldoTotal;
	}
	public BigDecimal getIdRediferido() {
		return idRediferido;
	}
	public void setIdRediferido(BigDecimal idRediferido) {
		this.idRediferido = idRediferido;
	}
	public String getRediferidoEspecial() {
		return rediferidoEspecial;
	}
	public void setRediferidoEspecial(String rediferidoEspecial) {
		this.rediferidoEspecial = rediferidoEspecial;
	}
	public String getOpcionesRediferido() {
		return opcionesRediferido;
	}
	public void setOpcionesRediferido(String opcionesRediferido) {
		this.opcionesRediferido = opcionesRediferido;
	}
	public BigDecimal getPagoMinimoInicial() {
		return pagoMinimoInicial;
	}
	public void setPagoMinimoInicial(BigDecimal pagoMinimoInicial) {
		this.pagoMinimoInicial = pagoMinimoInicial;
	}
	public BigDecimal getSaldoDolares() {
		return saldoDolares;
	}
	public void setSaldoDolares(BigDecimal saldoDolares) {
		this.saldoDolares = saldoDolares;
	}
	public BigDecimal getPagoMinDolares() {
		return pagoMinDolares;
	}
	public void setPagoMinDolares(BigDecimal pagoMinDolares) {
		this.pagoMinDolares = pagoMinDolares;
	}
	public BigDecimal getSaldoMoraDolares() {
		return saldoMoraDolares;
	}
	public void setSaldoMoraDolares(BigDecimal saldoMoraDolares) {
		this.saldoMoraDolares = saldoMoraDolares;
	}
	
	
}
