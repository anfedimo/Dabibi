<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:c="http://java.sun.com/jsp/jstl/core">
<body>
	<h:form>
		<p:panelGrid>
			<p:panel>
				<f:facet name="header">
		       Detalle de la gestión
			</f:facet>

				<p:row>
					<p:column colspan="4">
						<h:outputText
							value="Verifique que los datos registrados sean los correctos y luego haga clic en Guardar gestión." />
					</p:column>
				</p:row>

				<p:row>
					<p:column>
						<p:commandButton
							style="background-color:#F8991D;color:#fff;"
							value="Guardar gestión" icon="ui-icon-disk"
							onclick="guardarNuevaGestion('${politicaRediferidoBean.selectedTipificacion}')"
							action="#{gestionBean.guardarGestion}" 
							update="growl"/>
					</p:column>
					<p:column>
						<p:commandButton ajax="false" 
								action="#{inicioBean.regresarInicio}"
								style="background-color:#F8991D;color:#fff;"
								value="Finalizar gestión" icon="fa fa-send" />
					</p:column>
				</p:row>
				
				<p:row>
					<p:column colspan="4">
						<h:outputText value="Recuerde que habló con " />
						<h:outputText value="#{politicaRediferidoBean.fullNameAltitude}" />
						<h:outputText
							value=" del BANCO DAVIVIENDA. Cualquier información adicional al teléfono 7450446." />
					</p:column>
				</p:row>
			</p:panel>
		</p:panelGrid>

		<ui:include
			src="../../pages/obligacion/tablaHistoricoObligacion.xhtml" />
	</h:form>
</body>
</html>